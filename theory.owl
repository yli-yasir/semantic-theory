    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#BeingContrary">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Category"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#Belief -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Belief">
