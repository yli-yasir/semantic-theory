    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Games">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Fun"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Movies"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#Groups -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Groups">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Category"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#GuestUser -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#GuestUser">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#User"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#RegisteredUser"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#HelpingOthers -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#HelpingOthers">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Category"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#InvisibleRating -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#InvisibleRating">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Rating"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#hasVoteCount"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
