        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Rating"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#hasRegisteredSubmitter -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#hasRegisteredSubmitter">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#hasSubmitter"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#hasRelatedTheory -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#hasRelatedTheory">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Theory"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Theory"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#hasSubmitter -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#hasSubmitter">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#isSubmitterOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#isPhenomenaOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#isPhenomenaOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Phenomena"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Theory"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#isSubmitterOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#isSubmitterOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#User"/>
    </owl:ObjectProperty>
    


