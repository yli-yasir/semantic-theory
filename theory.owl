    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#hasDescription -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#hasDescription">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
