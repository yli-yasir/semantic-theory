    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#Category -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Category">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Attention"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Attribution"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Behavior"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#BeingContrary"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Belief"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Conforming"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#DecisionErrors"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#DecisionMaking"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Discomfort"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Emotion"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Forecasting"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Friendship"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Fun"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Groups"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#HelpingOthers"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Leadership"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Lies"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Meaning"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Memory"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Motivation"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Persuasion"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Power"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Resistance"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Science"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Trust"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Understanding"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#Comment -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Comment">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#hasSubmitter"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#User"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#hasContent"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#Conforming -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Conforming">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Category"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#CredibleUser -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#CredibleUser">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#RegisteredUser"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#hasRating"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#TopRated"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#DebatedTheory -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#DebatedTheory">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Theory"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#hasComment"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">20</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Comment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#DecisionErrors -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#DecisionErrors">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Category"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#DecisionMaking -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#DecisionMaking">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#Category"/>
