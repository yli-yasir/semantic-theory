    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#AcceptedTheory -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#AcceptedTheory">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Theory"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#hasRating"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#TopRated"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/yli-yasir/ontologies/theory#RejectedTheory"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#Attention -->

    <owl:Class rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#Attention">
