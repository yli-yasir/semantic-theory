    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/yli-yasir/ontologies/theory#hasName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/yli-yasir/ontologies/theory#hasName">
